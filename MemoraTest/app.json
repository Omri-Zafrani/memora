{
  "expo": {
    "name": "Memora",
    "slug": "MemoraTest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.memora.assistant",
      "icon": "./assets/icon.png",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "memora",
            "CFBundleURLSchemes": ["memora"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.memora.assistant",
      "icon": "./assets/icon.png",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "memora"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "memora",
    "extra": {
      "eas": {
        "projectId": "91627cdc-34c9-4cb9-95c1-34fc72d9eecc"
      }
    },
    "owner": "ofirmoshe"
  }
}
